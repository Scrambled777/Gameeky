<interface>
  <!-- interface-name actuators_row.ui -->
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.3"/>
  <template class="ActuatorsRow" parent="GtkBox">
    <property name="hexpand">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="AdwActionRow">
        <property name="subtitle">The actuators that will modify this entity behavior</property>
        <property name="title">Actuators</property>
        <child>
          <object class="GtkButton">
            <property name="halign">center</property>
            <property name="icon-name">list-add-symbolic</property>
            <property name="valign">center</property>
            <signal name="clicked" handler="on_clicked"/>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox" id="actuators">
        <property name="orientation">vertical</property>
      </object>
    </child>
    <style>
      <class name="actuators"/>
    </style>
  </template>
</interface>
