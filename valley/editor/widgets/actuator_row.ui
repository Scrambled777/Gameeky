<interface>
  <!-- interface-name actuator_row.ui -->
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="ActuatorRow" parent="AdwActionRow">
    <property name="hexpand">True</property>
    <child>
      <object class="GtkBox">
        <child>
          <object class="GtkDropDown" id="dropdown">
            <property name="model">
              <object class="GtkStringList" id="model">
                <property name="strings">activates
activates_i
affects
affects_i
aggroes
collapses
collapses_t
destroys
destroys_i
deteriorates
drops
exhausts
follows
interacts
propulses
requires
roams
spawns
takes
targets
teleports
teleports_i
transmutes
triggers
triggers_i
uses</property>
              </object>
            </property>
            <property name="selected">0</property>
            <property name="valign">center</property>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="child">
              <object class="GtkImage">
                <property name="icon-name">list-remove-symbolic</property>
              </object>
            </property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <signal name="clicked" handler="on_removed"/>
          </object>
        </child>
      </object>
    </child>
    <style>
      <class name="actuator"/>
    </style>
  </template>
</interface>
